+		bias-disable;
+	};
+
+	pinctrl_usbb_vbus_en: usbb-vbus-en {
+		pinmux = <PIN_PC20__GPIO>;
+		bias-disable;
+	};
+
+	pinctrl_usbc_vbus_en: usbc-vbus-en {
+		pinmux = <PIN_PC21__GPIO>;
