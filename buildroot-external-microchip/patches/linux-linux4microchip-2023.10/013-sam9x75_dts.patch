diff -Napur linux-linux4microchip-2023.10-orig/arch/arm/boot/dts/at91-sam9x75_curiosity_ksz9563.dts linux-linux4microchip-2023.10/arch/arm/boot/dts/at91-sam9x75_curiosity_ksz9563.dts
